{
  "name": "developer-center-front",
  "version": "1.0.0",
  "private": true,
  "description": " 开发者中心前端",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "build:prod": "cross-env UMI_ENV=prod MOCK=none umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:prod": "cross-env UMI_ENV=prod MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env UMI_ENV=pre umi dev",
    "start:test": "cross-env UMI_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-card": "^1.11.12",
    "@ant-design/pro-descriptions": "^1.7.7",
    "@ant-design/pro-form": "^1.22.1",
    "@ant-design/pro-layout": "^6.17.1",
    "@ant-design/pro-list": "^1.7.2",
    "@ant-design/pro-table": "^2.35.1",
    "ant-design-exframework": "^1.3.2",
    "antd": "^4.15.4",
    "array-move": "^3.0.1",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "numeral": "^2.0.6",
    "omit.js": "^2.0.2",
    "path-to-regexp": "2.4.0",
    "qs": "^6.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.9",
    "react-hotkeys-hook": "^3.3.1",
    "react-resizable": "^1.11.1",
    "react-sortable-hoc": "^2.0.0",
    "umi": "^3.4.14",
    "umi-request": "^1.3.5",
    "use-merge-value": "^1.0.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.4",
    "@types/classnames": "^2.3.1",
    "@types/express": "^4.17.11",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/mockjs": "^1.0.3",
    "@types/numeral": "^2.0.1",
    "@types/qs": "^6.9.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.1",
    "@types/react-resizable": "^1.7.2",
    "@umijs/fabric": "^2.5.12",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-qiankun": "^2.24.1",
    "@umijs/preset-ant-design-pro": "^1.3.0",
    "@umijs/preset-react": "^1.8.5",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/yorkie": "^2.0.5",
    "carlo": "^0.9.46",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.3.0",
    "detect-installer": "^1.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.25.0",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "pro-download": "^1.0.1",
    "puppeteer-core": "^5.5.0",
    "stylelint": "^13.13.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
